{
	"name": "imoracle",
	"version": "1.0.1",
	"description": "Node.js TypeScript Express - starter application",
	"main": "dist/server.js",
	"scripts": {
		"build": "npm run clear && npm run copy-assets && npm run build-ts",
		"test": "npm run test",
		"serve": "node dist/server.js",
		"build-ts": "tsc -p ./tsconfig.json",
		"copy-assets": "grunt dist",
		"clear": "ts-node ./static.ts",
		"tslint": "tslint -c tslint.json -p tsconfig.json"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/itorepo/imoracle.git"
	},
	"keywords": [
		"Node.js",
		"TypeScript",
		"Express",
		"starter",
		"skeleton",
		"template"
	],
	"author": "Kostyantyn Didenko <kdidenko@ito-global.com>",
	"license": "GPL-3.0",
	"bugs": {
		"url": "https://github.com/itorepo/imoracle/issues"
	},
	"homepage": "https://github.com/itorepo/imoracle#readme",
	"devDependencies": {
		"@types/express": "^4.11.1",
		"@types/node": "^9.6.4",
		"@types/supertest": "^2.0.4",
		"grunt": "^1.0.2",
		"grunt-contrib-clean": "^1.1.0",
		"grunt-contrib-copy": "^1.0.0",
		"supertest": "^3.0.0",
		"ts-node": "^5.0.1",
		"tslint": "^5.9.1",
		"typescript": "^2.8.1"
	},
	"dependencies": {
		"express": "^4.16.3",
		"pug": "^2.0.3",
		"shelljs": "^0.8.1"
	}
}
